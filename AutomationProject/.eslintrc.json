{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:cypress/recommended",
        "plugin:chai-friendly/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "no-unused-vars": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-underscore-dangle": 0
    }
}